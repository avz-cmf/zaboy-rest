{
    "name": "avz-cmf/zaboy-rest",
    "description": "REST resource as middleware",
    "type": "library",
    "license": "GPL-3.0",
    "keywords": [
        "php",
        "rest",
        "data store",
        "middleware"
    ],
    "homepage": "https://github.com/avz-cmf/zaboy-rest",
    "support": {
        "issues": "https://github.com/avz-cmf/zaboy-rest/issues",
        "source": "https://github.com/avz-cmf/zaboy-rest"
    },
    "require": {
        "zendframework/zend-servicemanager": "^3.0",
        "zendframework/zend-stratigility": "^1.1",
        "psr/http-message": "^1.0",
        "zendframework/zend-diactoros": "^1.1",
        "zendframework/zend-config": "^2.6",
        "xiag/rql-parser": "^1.0",
        "zendframework/zend-stdlib": "^3",
        "zendframework/zend-json": "^3",
        "zendframework/zend-db": "^2.8",
        "zendframework/zend-http": "^2.5",
        "symfony/filesystem": ">=2.7",
        "phpunit/phpunit": "^4.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "zaboy\\rest\\": "src/",
            "zaboy\\test\\rest\\": "test/src/"
